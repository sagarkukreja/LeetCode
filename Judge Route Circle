class Solution {
    public boolean judgeCircle(String moves) {
        int x = 0, y=0;
        char[] m = moves.toCharArray();
        for(char c:m){
            if(c=='U') y++;
            if(c=='D') y--;
            if(c=='L') x--;
            if(c=='R') x++;
        }
        return x==0 && y==0;
    }
}
